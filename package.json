{
  "name": "bazardemascotas",
  "version": "0.6.0",
  "description": "API for project-pets-client",
  "main": "server.js",
  "author": "javier gutierrez",
  "license": "MIT",
  "scripts": {
    "start": "yarn start:dev",
    "start:dev": "./node_modules/nodemon/bin/nodemon.js --exec babel-node src/app.ts --extensions '.ts'",
    "start:prod": "node dist/app.js",
    "build": "rm -rf dist && yarn build:js",
    "build:js": "babel src --out-dir dist --extensions \".ts\" --ignore \"src/**/*.test.ts\",\"src/**/*.test.js\" --source-maps inline",
    "test": "jest --watch --coverage",
    "type-check": "./node_modules/typescript/bin/tsc --noEmit",
    "type-check:watch": "yarn type-check -- --watch"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.4",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "mongoose": "^5.3.4",
    "morgan": "^1.9.1",
    "validator": "^10.8.0"
  },
  "engines": {
    "node": ">=10.x",
    "npm": ">=5.x",
    "yarn": ">=1.7.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-modules-commonjs": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "@types/bluebird": "^3.5.26",
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/helmet": "^0.0.43",
    "@types/jest": "^24.0.11",
    "@types/mongoose": "^5.3.26",
    "@types/morgan": "^1.7.35",
    "@types/supertest": "^2.0.7",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.7.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "jest": "^24.7.1",
    "nodemon": "^1.18.11",
    "regenerator-runtime": "^0.13.2",
    "supertest": "^4.0.2",
    "typescript": "^3.4.3"
  }
}
